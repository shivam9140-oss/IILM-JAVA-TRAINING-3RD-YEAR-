// Single Inheritance
 class vehicle { // parent
    void start() {
        System.out.println("Vehicle Starts");
    } 

}
class Bike extends vehicle { // child 
    void drive() { 
        System.out.println("Bike running");
	}
}

public class Main  
{
    public static void main(String[] args) {
        Bike b = new Bike();
        b.start();
        b.drive();
     }
}
//multi label inheritance

class Vehicle {   // Parent class
    void start() {
        System.out.println("Vehicle Starts");
    }
}

class Bike extends Vehicle {   // Child class
    void drive() {
        System.out.println("Bike running");
    }
}

class Bike2 extends Bike {     // Grandchild class
    void applyBrake() {
        System.out.println("Braking");
    }
}

public class Main {
    public static void main(String[] args) {
        Bike2 b = new Bike2();
        b.start();        // from Vehicle
        b.drive();        // from Bike
        b.applyBrake();   // from Bike2
    }
}
